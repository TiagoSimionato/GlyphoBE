programa
declare REAL b := 1R.
declare INTEIRO a.
declare BOOLEANO c := FALSO.
a := 2.
leia(a).
escreva(a).
se ((a) > a) entao {
  escreva(a)..
  leia(b).
}
ou se (c) entao {
  escreva(c)
}
senao {
  escreva(b).
}
enquanto (a <> 1) {
  a := a + 1.
  se (b <= 0.3R) entao {
    b := 3.0R  + 4.2R.
  }
}
fimprog.